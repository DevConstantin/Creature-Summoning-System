{
  "name": "Creature Summoning System",
  "tree": {
    "$className": "DataModel",
    "$ignoreUnknownInstances": true,

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "$ignoreUnknownInstances": true,
      "$path": "src/ServerScriptService"
    },

    "ServerStorage": {
      "$className": "ServerStorage",
      "$ignoreUnknownInstances": true,
      "$path": "src/ServerStorage"
    },
     
    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "$ignoreUnknownInstances": true,
      "$path": "src/ReplicatedStorage",
     
      "RemoteEvents": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true,
        "$path": "src/ReplicatedStorage/RemoteEvents",

        "CreatureSummonSystem": {
          "$className": "Folder",
          "$ignoreUnknownInstances": true,
          "$path": "src/ReplicatedStorage/RemoteEvents/CreatureSummonSystem",

          "SummonCreature": {
            "$className": "RemoteEvent",
            "$ignoreUnknownInstances": true,
            "$path": "src/ReplicatedStorage/RemoteEvents/CreatureSummonSystem/SummonCreature"
          },

          "RemoveCreature": {
            "$className": "RemoteEvent",
            "$ignoreUnknownInstances": true,
            "$path": "src/ReplicatedStorage/RemoteEvents/CreatureSummonSystem/RemoveCreature"
          },

          "FeedCreatureEvent": {
            "$className": "RemoteEvent",
            "$ignoreUnknownInstances": true,
            "$path": "src/ReplicatedStorage/RemoteEvents/CreatureSummonSystem/FeedCreatureEvent"
          },

          "ManageCreatureEvent": {
            "$className": "RemoteEvent",
            "$ignoreUnknownInstances": true,
            "$path": "src/ReplicatedStorage/RemoteEvents/CreatureSummonSystem/ManageCreatureEvent"
          }
        }
      },

      "Configurations": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true,
        "$path": "src/ReplicatedStorage/Configurations",

        "CreatureSystemsConfig": {
          "$className": "Configuration",
          "$ignoreUnknownInstances": true,
          "$path": "src/ReplicatedStorage/Configurations",
          "$properties": {
            "Attributes": {
              "MaxCreatures": {"Int32": 3},
              "InstructorOnly": {"Bool": true},
              "InteractRange": {"Int32": 12},
              "MaxDespawnTime": {"Int32": 60}
            }
          } 
        }
      }
    },

    "StarterGui": {
      "$className": "StarterGui",
      "$ignoreUnknownInstances": true,
      "$path": "src/StarterGui"
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "$ignoreUnknownInstances": true,
      "$path": "src/StarterPlayer"
    }
  }
}
